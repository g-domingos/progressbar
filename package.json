{
  "name": "progressbar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://g-domingos.github.io/progressbar",
  "scripts": {
    "dev:offline": "env-cmd -f .env.local npm run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "aws:deploy": "aws s3 sync dist/  s3://progressbar-integracomm-app --profile integracomm",
    "full:deploy": "env-cmd -f .env.development npm run build && aws s3 sync dist/  s3://progressbar-integracomm-app --profile integracomm && aws cloudfront create-invalidation --distribution-id E3EZ47PYHUL95R --paths /* --profile integracomm"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.4",
    "@types/node": "^18.19.39",
    "@types/react-datepicker": "^4.19.6",
    "aws": "^0.0.3-2",
    "aws-amplify": "^6.0.28",
    "axios": "^1.3.2",
    "bootstrap": "^5.2.3",
    "date-fns": "^2.30.0",
    "echarts-for-react": "^3.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^9.5.0",
    "framer-motion": "^11.0.20",
    "gh-pages": "^5.0.0",
    "google-api-client": "^2.1.0",
    "googleapis": "^110.0.0",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-datepicker": "^6.1.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.8.0",
    "react-select": "^5.8.0",
    "react-toastify": "^9.1.2",
    "styled-components": "^5.3.6",
    "ts-node": "^10.9.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^3.1.0",
    "github-pages": "^3.0.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.0"
  }
}
